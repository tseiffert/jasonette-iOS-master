                "{{#each Termine}}": {
                  "type": "horizontal",
                  "style": {
                    "padding": "5",
                    "spacing": "5"
                  },
                  "components": [
                    {
                      "type": "label",
                      "class": "label_class",
                      "text": "{{$root.GetDays(Tag,Monat,Stunde,Minute)}}",
                      "style": {
                        "width": "20%",
                        "color": "{{$root.GetColour(Tag,Monat,Stunde,Minute)}}"
                      }
                    },
                    
                    {
                      "type": "label",
                      "class": "label_class",
                      "text": "{{$root.GetHours(Stunde,Minute)}}",
                      "style": {
                        "width": "20%",
                        "color": "{{$root.GetColour(Tag,Monat,Stunde,Minute)}}"
                      }
                    },
                    {
                      "type": "label",
                      "class": "label_class",
                      "text": "{{Text}}",
                      "style": {
                        "width": "50%",
                        "color": "{{$root.GetColour(Tag,Monat,Stunde,Minute)}}"
                      }
                    }
                  ]
                }
              }
            }
          ],
